пакет  ru.mirea.pr3 ;

public  class  Square  extends  Rectangle {
    общественная  площадь () {}
    общественная  площадь ( двойная  сторона ) {
        это . ширина = сторона;
        это . длина = сторона;
    }
    общественный  квадрат ( цвет строки  , логическое заполнение , двойная сторона ) {  
        супер (цвет, заливка);
        это . ширина = сторона;
    }

    public  double  getSide () {
        ширина возврата ;
    }
    public  void  setSide ( двойная  сторона ) {
        это . ширина = сторона;
        это . длина = сторона;
    }

    @Override
    public  void  setWidth ( двойная  сторона ) {
        супер . setWidth (сторона);
    }

    @Override
    общественная  пустоты  SetLength ( двойная  сторона ) {
        супер . setLength (сторона);
    }

    @Override
    public  String  toString () {
        вернуть  " Square { "  +
                " сторона = "  + ширина +
                " , color = ' "  + color +  ' \ ' '  +
                ' } ' ;
    }
}
