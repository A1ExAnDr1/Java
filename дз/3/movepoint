пакет  ru.mirea.pr3 ;

открытый  класс  MovablePoint  реализует  Movable {
    int x, y, xSpeed, ySpeed;

    public  MovablePoint ( int  x , int  y , int  xSpeed , int  ySpeed ) {
        это . х = х;
        это . у = у;
        это . xSpeed = xSpeed;
        это . ySpeed = ySpeed;
    }

    @Override
    public  String  toString () {
        return  " MovablePoint { "  +
                « х = »  + х +
                " , y = "  + y +
                " , xSpeed ​​= "  + xSpeed +
                " , ySpeed ​​= "  + ySpeed +
                ' } ' ;
    }

    @Override
    public  void  moveUp () {
        y + = ySpeed;
    }

    @Override
    public  void  moveDown () {
        y - = ySpeed;
    }

    @Override
    public  void  moveLeft () {
        x - = xSpeed;
    }

    @Override
    public  void  moveRight () {
        x + = xSpeed;
    }
}
